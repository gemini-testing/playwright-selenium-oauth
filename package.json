{
    "name": "playwright-selenium-oauth",
    "version": "1.0.2",
    "description": "Plugin for convenient setting of authorization OAuth token header for PlayWright - works with Selenium",
    "main": "build/index.js",
    "files": [
        "build"
    ],
    "scripts": {
        "build": "tsc --build",
        "clean": "rimraf build/",
        "eslint": "eslint src --config '.eslintrc.js' --ignore-path '.eslintignore'",
        "lint": "npm run eslint && npm run style",
        "prepack": "npm run clean && npm run build",
        "prettier": "prettier 'src/**/*.ts' '**/*.json' --ignore-path '.prettierignore'",
        "reformat": "npm run eslint -- --fix && npm run prettier -- --write",
        "style": "npm run prettier -- --check",
        "unit": "jest --config 'jest.config.js'",
        "test": "npm run lint && npm run unit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gemini-testing/playwright-oauth.git"
    },
    "engines": {
        "node": ">= 16"
    },
    "keywords": [
        "playwright",
        "oauth"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gemini-testing/playwright-oauth/issues"
    },
    "homepage": "https://github.com/gemini-testing/playwright-oauth#readme",
    "author": "Rostislav Provodenko",
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "@types/node": "^16.18.36",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "eslint-config-gemini-testing": "^3.0.0",
        "eslint-config-prettier": "^8.8.0"
    }
}
